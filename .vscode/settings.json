{
  "git.ignoreLimitWarning": true,
  "editor.formatOnSave": true,
  "editor.fontSize": 18,
  "workbench.colorTheme": "Winter is Coming (Dark Blue - No Italics)",
  "editor.fontFamily": "JetBrains Mono",
  "auto-rename-tag.activationOnLanguage": ["html", "xml", "php", "javascript"],
  "javascript.suggest.autoImports": true,
  "extension.changeCase.commands": true,
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "connectString": "postgres://usbwbd_integrispan:postgrespassword@localhost:5432/sbwbd",
      "driver": "PostgreSQL",
      "name": "IntegriSpan"
    }
  ],
  "cSpell.words": [
    "antd",
    "Cascader",
    "CHOKIDAR",
    "dgraph",
    "dind",
    "gitlab",
    "grahpql",
    "hasura",
    "Inps",
    "Inpusts",
    "Integri",
    "Integridutos",
    "INTEGRIDUTOS",
    "integrispan",
    "postgres",
    "rrdls",
    "subweb",
    "typeorm",
    "USEPOLLING",
    "WEVERTONM"
  ],
  "editor.formatOnType": true,
  "formattingToggle.affects": ["formatOnSave"],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "npm-intellisense.importES6": true,
  "npm-intellisense.importQuotes": "'",
  "npm-intellisense.importLinebreak": ";\r\n",
  "npm-intellisense.importDeclarationType": "const",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true,
      "source.organizeImports": true
    }
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#a396f3",
    "activityBar.activeBorder": "#c83017",
    "activityBar.background": "#a396f3",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#c83017",
    "activityBarBadge.foreground": "#e7e7e7",
    "sash.hoverBorder": "#a396f3",
    "statusBar.background": "#7b68ee",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#a396f3",
    "statusBarItem.remoteBackground": "#7b68ee",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#7b68ee",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#7b68ee99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#7B68EE"
}
